<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
   http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>StudentPage</servlet-name>
    <servlet-class>controller.StudentPage</servlet-class>

  </servlet>
  <servlet-mapping>

    <servlet-name>StudentPage</servlet-name>
    <url-pattern>/StudentPage</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>addStudentServlet</servlet-name>
    <servlet-class>controller.addStudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>

    <servlet-name>addStudentServlet</servlet-name>
    <url-pattern>/addStudentServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>editStatus</servlet-name>
    <servlet-class>controller.editStatus</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editStatus</servlet-name>
    <url-pattern>/editStatus</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editStudent</servlet-name>
    <servlet-class>controller.editStudent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editStudent</servlet-name>
    <url-pattern>/editStudent</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>addMark</servlet-name>
    <servlet-class>controller.addMark</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addMark</servlet-name>
    <url-pattern>/addMark</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <el-ignored>false</el-ignored>
    </jsp-property-group>
  </jsp-config>
</web-app>
